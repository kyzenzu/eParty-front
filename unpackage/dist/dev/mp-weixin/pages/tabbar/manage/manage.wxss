page {
  height: 100%;
  background-color: #F6F6F6;
}
.container {
  box-sizing: border-box;
}
.container .filter {
  width: 100%;
  height: 89rpx;
  padding: 20rpx 33rpx;
  box-sizing: border-box;
  display: flex;
  text-align: center;
  justify-content: space-between;
}
.container .filter text {
  font-size: 30rpx;
}
.container .filter .filter-box {
  width: 179rpx;
  height: 49rpx;
  background-color: #FFF;
  border-radius: 24.5rpx;
  border: solid 1rpx #000;
}
.container .filter .filter-box .picker-inner {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .filter .filter-box .picker-inner .filter-text {
  font-size: 26rpx;
  line-height: 49rpx;
  letter-spacing: 3rpx;
}
.container .filter .filter-box .picker-inner image {
  margin-left: 12rpx;
  width: 18.26rpx;
  height: 10.58rpx;
}
.container .contents {
  box-sizing: border-box;
  padding: 0 20rpx;
  padding-bottom: calc(0px + 28rpx);
}
.container .contents .content-card {
  width: 100%;
  background-color: #FFF;
  padding-bottom: 50rpx;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}
.container .contents .content-card .title {
  padding-top: 25rpx;
  padding-left: 25rpx;
  font-weight: bold;
  letter-spacing: 5rpx;
}
.container .contents .content-card .subtitle {
  padding-top: 10rpx;
  text-align: center;
  font-size: 20rpx;
}
.container .contents .content-card .charts-box {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.container .contents .content-card .charts-box.funnel-charts {
  height: 90%;
}
.container .contents .member-account {
  height: 538rpx;
}
.container .contents .join-condition {
  height: 538rpx;
}
.container .contents .composition-box {
  height: 480rpx;
}
.container .contents .composition-box .compositions {
  padding-top: 75rpx;
  padding-left: 26rpx;
  padding-right: 26rpx;
  white-space: nowrap;
  box-sizing: border-box;
}
.container .contents .composition-box .compositions .branch {
  width: 421rpx;
  height: 248rpx;
  background-color: #67D6D0;
  display: inline-block;
  margin-right: 75rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
}
.container .contents .composition-box .compositions .branch .branch-info {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.container .contents .composition-box .compositions .branch .branch-info .branch-name {
  font-size: 40rpx;
  color: #FFF;
  letter-spacing: 3rpx;
}
.container .contents .composition-box .compositions .branch .branch-info .branch-details {
  font-size: 20rpx;
  color: #FFF;
}
.container .contents .approval-mterials {
  height: 592rpx;
}

