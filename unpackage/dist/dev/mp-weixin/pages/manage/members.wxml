<view class="members-page"><view class="page-header"><view class="header-left"><image class="party-icon _img" src="static/images/manage/party-yellow.png" alt></image><text class="header-text">树莓22-2入党申请人信息</text></view><text class="page-number">25/31</text></view><view class="member-list"><block wx:for="{{memberList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="member-item"><view class="member-info"><view class="basic-row"><image class="avatar" src="{{item.avatar}}" mode="aspectFill"></image><view class="info-content"><view class="name-row"><text class="name">{{item.name}}</text></view><view class="basic-info"><text class="age">{{item.age+"岁"}}</text><text class="gender">{{item.gender}}</text><text class="ethnicity">{{item.ethnicity}}</text></view></view><text class="edit-btn">查看</text></view><view class="status-row"><text class="status-label">当前进度：</text><text class="status-text">{{item.status}}</text><view class="star-rating"><block wx:for="{{5}}" wx:for-item="n" wx:for-index="__i0__" wx:key="*this"><text class="{{['star',n<=item.rating?'filled':'']}}">★</text></block></view></view></view></view></block></view></view>